@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Nonuser.cshtml";
}
<div class="col-lg-12">
    <div class="col-lg-6">
        <div class="col-lg-3">
            <h4>Customer Code :</h4>
        </div>
        <div class="col-lg-3">
            <h4><label id="customercode"></label></h4>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="col-lg-3">
            <h4>Customer Name :</h4>
        </div>
        <div class="col-lg-6">
            <h4><label id="customername"></label></h4>
        </div>
    </div>
</div>

<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            Customer Ticket Detail

        </div>
        <!-- /.panel-heading -->
        <div class="panel-body">
            <table class="table table-striped table-bordered table-hover" id="dataTables-examp" style="width: 100%!important">
                <thead>
                    <tr>
                        <th>Ticket Number</th>
                        <th>Name</th>
                        <th>Project</th>
                        <th>Branch Code / Location</th>
                        <th>Description</th>
                        <th>Date</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody id="dynamic-Grid"></tbody>
            </table>
            <!-- /.table-responsive -->
        </div>
        <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
</div>
<!-- /.col-lg-12 -->
<!-- /.row -->
<input action="action" onclick="history.go(-1);" type="button" value="Back" class="btn btn-primary" id="back" />

<script type="text/javascript">

    $(document).ready(function () {
        $.ajax({
            async: false,
            url: '/api/CheckStatusApi/',
            type: 'POST',
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (data) {
                console.log(data);
                $("#dataTables-examp").DataTable({
                    "bDestroy": true,
                    "aaData": data.list,
                    "aoColumns": [
                         { "mDataProp": "tid" },
                         { "mDataProp": "name" },
                         { "mDataProp": "project_name" },
                         { "mDataProp": "branch_code" },
                         { "mDataProp": "description" },
                         { "mDataProp": "date" },
                         { "mDataProp": "status" }
                    ]
                });
                document.getElementById('customercode').innerHTML = data.code;
                document.getElementById('customername').innerHTML = data.customer_name;
            },
            error: function () { alert('Data Not Found'); }
        });
    });


    function filltable(datas) {

    }
</script>