@{
    ViewBag.Title = "ViewInventory";
    if (ViewData["id"].ToString() == "1")
    {
        Layout = "~/Views/Shared/_Home.cshtml";
    }
    else if (ViewData["id"].ToString() == "2")
    {
        Layout = "~/Views/Shared/Admin.cshtml";
    }
    else if (ViewData["id"].ToString() == "3")
    {
        Layout = "~/Views/Shared/Normal.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/CSR.cshtml";
    }
}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Product Details</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Pro Detail
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Type</th>
                                <th>Catogery</th>
                                <th>Brand</th>
                                <th>Model</th>                          
                                <th>Barcode</th>
                                <th>Inward Date</th>
                                <th>Desc</th>
                                <th>Quantity</th>
                                <th>Vendor Name</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody id="dynamic-Grid"></tbody>
                    </table>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
        <!-- /.row -->
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            async: false,
            url: '/api/InventoryApi/',
            type: 'PUT',
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (data) {
                console.log(data);
                filltable(data)
                //var type;
                //for (var i = 0; i < data.length; i++) {
                //    if (data[i].type == true) {
                //        type = "Local"
                //    }
                //    else {
                //        type = "Imported";

                //    }
                //    $("#dynamic-Grid").append("<tr id=" + i + "><td>" + data[i].name + "</td><td>" + type + "</td><td>" + data[i].catogrey + "</td><td>" + data[i].brand + "</td><td>" + data[i].model + "</td><td>" + data[i].barcode + "</td><td>" + data[i].description + "</td><td>" + data[i].quantity + "</td><td>" + data[i].vendorName + "</td><td><button value='Update' class='btn btn-primary fa fa-cog' id='upd' onclick=\"preUpdate(" + data[i].id + ")\"></button> <button  class='btn btn-danger fa fa-times' onclick=\"deleteUser(" + data[i].id + "," + i + ")\"></button></td></tr>");
                //}
            },
            error: function () { alert('Grid Not Load'); }
        });
    });
    function filltable(datas) {
        $("#dataTables-example").DataTable({
            "aaData": datas,
            "aoColumns": [
                { "mDataProp": "name" },
                { "mDataProp": "type" },
                { "mDataProp": "catogrey" },
                { "mDataProp": "brand" },
                { "mDataProp": "model" },
                { "mDataProp": "barcode" },
                { "mDataProp": "inward" },
                { "mDataProp": "description" },
                { "mDataProp": "quantity" },
                { "mDataProp": "vendorName" },
                { "mDataProp": "action" }
            ]
        });
    }
    function Detailproduct(id) {
        $.ajax({
            async: false,
            url: '/api/InventoryApi/' + id,
            type: 'POST',
            success: function (data) {
                console.log(data);
                window.location.href = '/Inventory/ViewInventoryDetail';
                location.reload();
            },
            error: function () {
                alert('User not Updated');
            }
        });
    }
</script>