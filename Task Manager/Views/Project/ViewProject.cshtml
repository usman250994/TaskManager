@{
    ViewBag.Title = "ViewProject";
    if (ViewData["id"].ToString() == "1")
    {
        Layout = "~/Views/Shared/_Home.cshtml";
    }
    else if (ViewData["id"].ToString() == "2")
    {
        Layout = "~/Views/Shared/Admin.cshtml";
    }
    else if (ViewData["id"].ToString() == "3")
    {
        Layout = "~/Views/Shared/Normal.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/CSR.cshtml";
    }
}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Project Detail</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Project Detail
                    @*<button type="button" class="btn btn-primary" onclick="gridLoad()">Update</button>*@
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Start Date</th>
                                <th>End Date</th>
                                <th>Customer </th>
                                <th>Resco Manager</th>
                                <th>Customer Manager</th>
                                <th>Customer Email</th>
                                <th>Customer Mobile</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody id="dynamic-Grid"></tbody>
                    </table>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
        <!-- /.row -->
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            async:false,
            url: '/api/ProjectApi/',
            type: 'GET',
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (data) {
                console.log(data);
                for (var i = 0; i < data.length; i++) {
                    $("#dynamic-Grid").append("<tr id="+i+"><td>" + data[i].projectName + "</td><td>" + data[i].startDate.substring(0, 10) + "</td><td>" + data[i].endDate.substring(0, 10) + "</td><td>" + data[i].customerName + "</td><td>" + data[i].projectManager + "</td><td>" + data[i].customerManager + "</td><td>" + data[i].Email + "</td><td>" + data[i].PhoneNumber + "</td><td><button value='Update' class='btn btn-primary fa fa-cog' id='upd' onclick=\"preUpdate(" + data[i].id + ")\"></button> <button  class='btn btn-danger fa fa-times' onclick=\"deleteUser(" + data[i].id +","+i+")\"></button></td></tr>");
                   
                }
            },
            error: function () { alert('Grid Not Load'); }
        });
    });



    function deleteUser(id,rId) {
        $.ajax({
            url: '/api/ProjectApi/' + id,
            type: 'POST',
            contentType: "application/json;charset=utf-8",
            success: function (data) {
                alert(data);

                $('table#dataTables-example tr#' + rId).remove();
            },
            error: function (data) { alert(data); }
        });
    }
    function preUpdate(id) {

        $.ajax({
            async: false,
            url: '/api/ProjectApi/' + id,
            type: 'PUT',
            success: function (data) {
                window.location.href = '/Project/CreateProject';
            },
            error: function () {
                alert('User not Updated');
            }
        });
    }
</script>